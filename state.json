inite alternate;\n}\n\n@keyframes textGlow {\n  from { \n    text-shadow: \n      0 0 20px rgba(0,255,136,0.5),\n      0 0 40px rgba(0,255,136,0.3);\n  }\n  to { \n    text-shadow: \n      0 0 30px rgba(0,255,136,0.7),\n      0 0 60px rgba(0,255,136,0.4),\n      0 0 80px rgba(0,255,136,0.2);\n  }\n}\n\n.cursor {\n  display: inline-block;\n  width: 4px;\n  height: 1em;\n  background: #00ff88;\n  margin-left: 0.2em;\n  animation: blink 1s step-end infinite;\n  vertical-align: baseline;\n}\n\n@keyframes blink {\n  0%, 100% { opacity: 1; }\n  50% { opacity: 0; }\n}\n\n.subtext {\n  font-family: 'Space Mono', monospace;\n  font-size: clamp(0.7rem, 1.8vw, 0.9rem);\n  color: rgba(0,255,136,0.4);\n  margin-top: 1.5rem;\n  letter-spacing: 0.15em;\n}\n\n/* Floating data streams */\n.stream {\n  position: absolute;\n  font-family: 'Space Mono', monospace;\n  font-size: 0.6rem;\n  color: rgba(0,255,136,0.15);\n  writing-mode: vertical-rl;\n  animation: streamDown 20s linear infinite;\n}\n\n.stream:nth-child(1) { left: 8%; animation-duration: 15s; }\n.stream:nth-child(2) { left: 25%; animation-duration: 22s; animation-delay: -5s; }\n.stream:nth-child(3) { right: 20%; animation-duration: 18s; animation-delay: -10s; }\n.stream:nth-child(4) { right: 8%; animation-duration: 25s; animation-delay: -3s; }\n\n@keyframes streamDown {\n  from { transform: translateY(-100%); }\n  to { transform: translateY(100vh); }\n}\n\n/* Corner accents */\n.corner {\n  position: absolute;\n  width: 30px;\n  height: 30px;\n  border: 1px solid rgba(0,255,136,0.3);\n}\n\n.corner.tl { top: 15%; left: 10%; border-right: none; border-bottom: none; }\n.corner.tr { top: 15%; right: 10%; border-left: none; border-bottom: none; }\n.corner.bl { bottom: 15%; left: 10%; border-right: none; border-top: none; }\n.corner.br { bottom: 15%; right: 10%; border-left: none; border-top: none; }\n</style>\n\n<div class=\"canvas-container\">\n  <div class=\"grid\"></div>\n  \n  <div class=\"stream\">01001111 01001110</div>\n 