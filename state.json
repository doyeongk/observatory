  "html": "<style>\n@import url('https://fonts.googleapis.com/css2?family=Crimson+Pro:ital,wght@0,200;0,300;1,200;1,300&family=Syne:wght@400;700&display=swap');\n\n.canvas-container {\n  height: 100%;\n  width: 100%;\n  background: #F2EDE4;\n  position: relative;\n  overflow: hidden;\n}\n\n/* Linen texture overlay */\n.canvas-container::before {\n  content: '';\n  position: absolute;\n  inset: 0;\n  background-image: url(\"data:image/svg+xml,%3Csvg viewBox='0 0 200 200' xmlns='http://www.w3.org/2000/svg'%3E%3Cfilter id='noise'%3E%3CfeTurbulence type='fractalNoise' baseFrequency='1.2' numOctaves='3' stitchTiles='stitch'/%3E%3C/filter%3E%3Crect width='100%25' height='100%25' filter='url(%23noise)'/%3E%3C/svg%3E\");\n  opacity: 0.035;\n  pointer-events: none;\n  z-index: 50;\n}\n\n/* Warm wash at the top â€” morning light flooding in */\n.light-wash {\n  position: absolute;\n  top: -20%;\n  left: -10%;\n  right: -10%;\n  height: 70%;\n  background: radial-gradient(\n    ellipse 100% 100% at 60% 0%,\n    rgba(218, 170, 110, 0.18) 0%,\n    rgba(218, 170, 110, 0.08) 35%,\n    transparent 65%\n  );\n  animation: washPulse 10s ease-in-out infinite;\n}\n\n@keyframes washPulse {\n  0%, 100% { opacity: 0.8; }\n  50% { opacity: 1; }\n}\n\n/* The loom â€” vertical warp threads */\n.warp {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  width: 1px;\n  opacity: 0;\n  animation: warpAppear 2s ease-out forwards;\n}\n\n.warp:nth-child(1)  { left: 8%;  background: rgba(196, 156, 104, 0.25); animation-delay: 0.1s; }\n.warp:nth-child(2)  { left: 14%; background: rgba(196, 156, 104, 0.15); animation-delay: 0.2s; }\n.warp:nth-child(3)  { left: 20%; background: rgba(196, 156, 104, 0.30); animation-delay: 0.05s; }\n.warp:nth-child(4)  { left: 26%; background: rgba(196, 156, 104, 0.12); animation-delay: 0.35s; }\n.warp:nth-child(5)  { left: 32%; background: rgba(196, 156, 104, 0.22); animation-delay: 0.15s; }\n.warp:nth-child(6)  { left: 38%; background: rgba(196, 156, 104, 0.18); animation-delay: 0.4s; }\n.warp:nth-child(7)  { left: 44%; background: rgba(196, 156, 104, 0.35); animation-delay: 0.08s; width: 2px; }\n.warp:nth-child(8)  { left: 50%; background: rgba(196, 156, 104, 0.14); animation-delay: 0.28s; }\n.warp:nth-child(9)  { left: 56%; background: rgba(196, 156, 104, 0.20); animation-delay: 0.18s; }\n.warp:nth-child(10) { left: 62%; background: rgba(196, 156, 104, 0.28); animation-delay: 0.12s; }\n.warp:nth-child(11) { left: 68%; background: rgba(196, 156, 104, 0.10); animation-delay: 0.45s; }\n.warp:nth-child(12) { left: 74%; background: rgba(196, 156, 104, 0.24); animation-delay: 0.22s; }\n.warp:nth-child(13) { left: 80%; background: rgba(196, 156, 104, 0.16); animation-delay: 0.32s; }\n.warp:nth-child(14) { left: 86%; background: rgba(196, 156, 104, 0.30); animation-delay: 0.06s; }\n.warp:nth-child(15) { left: 92%; background: rgba(196, 156, 104, 0.12); animation-delay: 0.38s; }\n\n@keyframes warpAppear {\n  0% { opacity: 0; transform: scaleY(0); transform-origin: top; }\n  100% { opacity: 1; transform: scaleY(1); }\n}\n\n/* The weft â€” horizontal thread weaving through */\n.weft {\n  position: absolute;\n  left: 0;\n  right: 0;\n  height: 2px;\n  background: linear-gradient(\n    90deg,\n    transparent 0%,\n    rgba(194, 114, 88, 0.0) 5%,\n    rgba(194, 114, 88, 0.5) 15%,\n    rgba(194, 114, 88, 0.7) 30%,\n    rgba(194, 114, 88, 0.5) 50%,\n    rgba(194, 114, 88, 0.7) 70%,\n    rgba(194, 114, 88, 0.5) 85%,\n    rgba(194, 114, 88, 0.0) 95%,\n    transparent 100%\n  );\n  opacity: 0;\n  animation: weftWeave 4s ease-in-out forwards;\n}\n\n.weft-1 { top: 38%; animation-delay: 1.2s; }\n.weft-2 { top: 50%; animation-delay: 1.8s; height: 3px; }\n.weft-3 { top: 62%; animation-delay: 2.4s; }\n\n@keyframes weftWeave {\n  0% { \n    opacity: 0;\n    clip-path: inset(0 100% 0 0);\n  }\n  60% {\n    clip-path: inset(0 0 0 0);\n    opacity: 0.8;\n  }\n  100% {\n    clip-path: inset(0 0 0 0);\n    opacity: 0.6;\n  }\n}\n\n/* Intersection glow â€” where warp meets weft */\n.node {\n  position: absolute;\n  width: 6px;\n  height: 6px;\n  border-radius: 50%;\n  background: rgba(218, 170, 110, 0.8);\n  box-shadow: 0 0 12px rgba(218, 170, 110, 0.4), 0 0 30px rgba(218, 170, 110, 0.15);\n  opacity: 0;\n  animation: nodeGlow 3s ease-in-out forwards;\n}\n\n.node:nth-child(1)  { left: calc(20% - 3px); top: calc(38% - 3px); animation-delay: 2.0s; }\n.node:nth-child(2)  { left: calc(44% - 3px); top: calc(50% - 3px); animation-delay: 2.5s; width: 8px; height: 8px; }\n.node:nth-child(3)  { left: calc(62% - 3px); top: calc(38% - 3px); animation-delay: 2.2s; }\n.node:nth-child(4)  { left: calc(74% - 3px); top: calc(62% - 3px); animation-delay: 2.8s; }\n.node:nth-child(5)  { left: calc(32% - 3px); top: calc(62% - 3px); animation-delay: 3.0s; }\n.node:nth-child(6)  { left: calc(86% - 3px); top: calc(50% - 3px); animation-delay: 2.6s; }\n.node:nth-child(7)  { left: calc(50% - 3px); top: calc(62% - 3px); animation-delay: 3.2s; }\n\n@keyframes nodeGlow {\n  0% { opacity: 0; transform: scale(0); }\n  50% { opacity: 1; transform: scale(1.3); }\n  100% { opacity: 0.7; transform: scale(1); }\n}\n\n/* Central node breathes */\n.node:nth-child(2) {\n  animation: nodeGlow 3s ease-in-out forwards, nodeBreathe 5s ease-in-out 3s infinite;\n}\n\n@keyframes nodeBreathe {\n  0%, 100% { \n    box-shadow: 0 0 12px rgba(218, 170, 110, 0.4), 0 0 30px rgba(218, 170, 110, 0.15);\n    transform: scale(1);\n  }\n  50% { \n    box-shadow: 0 0 20px rgba(218, 170, 110, 0.6), 0 0 50px rgba(218, 170, 110, 0.25);\n    transform: scale(1.15);\n  }\n}\n\n/* Text â€” left-anchored, editorial */\n.text-block {\n  position: absolute;\n  left: 8%;\n  bottom: 14%;\n  z-index: 10;\n}\n\n.title {\n  font-family: 'Crimson Pro', serif;\n  font-weight: 200;\n  font-style: italic;\n  font-size: clamp(4.5rem, 14vw, 8.5rem);\n  color: rgba(61, 43, 31, 0.55);\n  letter-spacing: 0.04em;\n  line-height: 0.9;\n  opacity: 0;\n  animation: titleIn 2s ease-out 0.8s forwards;\n}\n