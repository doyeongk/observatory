 canvas.width = window.innerWidth;\n  h = canvas.height = window.innerHeight;\n}\n\nfunction initAudio() {\n  if (audioStarted) return;\n  audioStarted = true;\n  startEl.style.display = 'none';\n  \n  audioCtx = new (window.AudioContext || window.webkitAudioContext)();\n  \n  // Two oscillators for X and Y\n  osc1 = audioCtx.createOscillator();\n  osc2 = audioCtx.createOscillator();\n  gain = audioCtx.createGain();\n  \n  osc1.type = 'sine';\n  osc2.type = 'sine';\n  gain.gain.value = 0.03; // quiet\n  \n  // Analyzer for getting waveform data\n  window.analyserX = audioCtx.createAnalyser();\n  window.analyserY = audioCtx.createAnalyser();\n  analyserX.fftSize = 2048;\n  analyserY.fftSize = 2048;\n  \n  osc1.connect(analyserX);\n  osc2.connect(analyserY);\n  osc1.connect(gain);\n  osc2.connect(gain);\n  gain.connect(audioCtx.destination);\n  \n  osc1.start();\n  osc2.start();\n}\n\nfunction updateFrequencies() {\n  if (!audioStarted) return;\n  \n  // Base frequency affected by mouse Y\n  const baseFreq = 100 + mouseY * 300;\n  \n  // Get current ratio\n  const [r1, r2] = ratios[currentRatio];\n  \n  // Apply ratio for Lissajous pattern\n  osc1.frequency.setTargetAtTime(baseFreq * r1, audioCtx.currentTime, 0.1);\n  osc2.frequency.setTargetAtTime(baseFreq * r2, audioCtx.currentTime, 0.1);\n}\n\nfunction getLissajousPoint(t) {\n  if (!audioStarted) {\n    // Demo mode before audio\n    const [r1, r2] = ratios[currentRatio];\n    const baseFreq = 2;\n    const x = Math.sin(t * baseFreq * r1 + phase);\n    const y = Math.sin(t * baseFreq * r2);\n    return { x, y };\n  }\n  \n  // Get actual audio data\n  const bufferLength = analyserX.fftSize;\n  const dataX = new Float32Array(bufferLength);\n  const dataY = new Float32Array(bufferLength);\n  analyserX.getFloatTimeDomainData(dataX);\n  analyserY.getFloatTimeDomainData(dataY);\n  \n  // Sample at position based on t\n  const idx = Math.floor((t % 1) * bufferLength);\n  return {\n    x: dataX[idx] || 0,\n    y: dataY[idx] || 0\n  };\n}\n\nfunction draw(timestamp) {\n  // Fade effect\n  ctx.fillStyle = 'rgba(0, 0, 0, 0.08)';\n  ctx.fillRect(0, 0, w, h);\n  \n  const cx = w / 2;\n  const cy = h / 2;\n  const scale = Math.min(w, h) * 0.35;\n  \n  // Update ratio periodically\n  ratioTimer++;\n  if (ratioTimer > 500) {\n    ratioTimer = 0;\n    currentRatio = (currentRatio + 1) % ratios.length;\n    updateFrequencies();\n  }\n  \n  // Phase shift for animation\n  phase += 0.02 + mouseX * 0.03;\n  \n  // Draw multiple samples\n  const samples = 150;\n  const t = timestamp * 0.001;\n  \n  for (let i = 0; i < samples; i++) {\n    const sampleT = t + i * 0.02;\n    const point = getLissajousPoint(sampleT);\n    \n    const x = cx + point.x * scale;\n    const y = cy + point.y * scale;\n    \n    // Add to history\n    history.push({ x, y, age: 0 });\n    if (history.length > historyLength) history.shift();\n  }\n  \n  // Draw history with gradient\n  for (let i = 0; i < history.length; i++) {\n    const p = history[i];\n    p.age++;\n    \n    const life = 1 - (p.age / historyLength);\n    if (life <= 0) continue;\n    \n    // Color shifts based on position\n    const hue = (p.x / w) * 60 + 280; // purple to pink range\n    const lightness = 50 + life * 30;\n    \n    // Cyan core, pink edges\n    const distFromCenter = Math.sqrt((p.x - cx) ** 2 + (p.y - cy) ** 2) / scale;\n    const isCyan = distFromCenter < 0.5;\n    \n    if (isCyan) {\n      ctx.fillStyle = `hsla(180, 100%, ${lightness}%, ${life * 0.8})`;\n    } else {\n      ctx.fillStyle = `hsla(${hue}, 100%, ${lightness}%, ${life * 0.6})`;\n    }\n    \n    const size = life * 2.5;\n    ctx.beginPath();\n    ctx.arc(p.x, p.y, size, 0, Math.PI * 2);\n    ctx.fill();\n  }\n  \n  // Bright current point\n  const current = getLissajousPoint(t);\n  const currentX = cx + current.x * scale;\n  const currentY = cy + current.y * scale;\n  \n  // Glow\n  const gradient = ctx.createRadialGradient(currentX, currentY, 0, currentX, currentY, 20);\n  gradient.addColorStop(0, 'rgba(0, 255, 255, 0.8)');\n  gradient.addColorStop(0.5, 'rgba(255, 0, 255, 0.3)');\n  gradient.addColorStop(1, 'transparent');\n  ctx.fillStyle = gradient;\n  ctx.beginPath();\n  ctx.arc(currentX, currentY, 20, 0, Math.PI * 2);\n  ctx.fill();\n  \n  // Core\n  ctx.fillStyle = '#fff';\n  ctx.beginPath();\n  ctx.arc(currentX, currentY, 3, 0, Math.PI * 2);\n  ctx.fill();\n  \n  // Frequency display\n  if (audioStarted) {\n    const [r1, r2] = ratios[currentRatio];\n    ctx.fillStyle = 'rgba(0, 255, 255, 0.4)';\n    ctx.font = '12px monospace';\n    ctx.fillText(`ratio ${r1}:${r2}`, 20, h - 20);\n    ctx.fillText(`freq ${Math.round(osc1.frequency.value)}hz / ${Math.round(osc2.frequency.value)}hz`, 20, h - 40);\n  }\n  \n  requestAnimationFrame(draw);\n}\n\n// Mouse interaction\ndocument.addEventListener('mousemove', (e) => {\n  mouseX = e.clientX / w;\n  mouseY = e.clientY / h;\n  updateFrequencies();\n});\n\ndocument.addEventListener('click', initAudio);\n\n// Touch support\ndocument.addEventListener('touchstart', (e) => {\n  initAudio();\n  const touch = e.touches[0];\n  mouseX = touch.clientX / w;\n  mouseY = touch.clientY / h;\n  updateFrequencies();\n});\n\ndocument.addEventListener('touchmove', (e) => {\n  const touch = e.touches[0];\n  mouseX = touch.clientX / w;\n  mouseY = touch.clientY / h;\n  updateFrequencies();\n});\n\nwindow.addEventListener('resize', resize);\nresize();\nrequestAnimationFrame(draw);\n</script>\n</body>\n</html>\n"
