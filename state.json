gin: 1.5rem auto;\n  opacity: 0;\n  animation: draw 1.5s ease-out 1s forwards;\n}\n\n@keyframes draw {\n  from {\n    width: 0;\n    opacity: 0;\n  }\n  to {\n    width: 60px;\n    opacity: 0.6;\n  }\n}\n\n.subtitle {\n  font-family: 'Cormorant Garamond', serif;\n  font-size: clamp(0.9rem, 2.5vw, 1.1rem);\n  font-weight: 300;\n  font-style: italic;\n  color: #6b5d4d;\n  letter-spacing: 0.25em;\n  text-transform: lowercase;\n  opacity: 0;\n  animation: fade 2s ease-out 1.5s forwards;\n}\n\n@keyframes fade {\n  from { opacity: 0; }\n  to { opacity: 0.7; }\n}\n\n/* Floating dust motes */\n.mote {\n  position: absolute;\n  width: 3px;\n  height: 3px;\n  background: radial-gradient(circle, rgba(255,235,200,0.9) 0%, transparent 70%);\n  border-radius: 50%;\n  animation: float 12s ease-in-out infinite;\n}\n\n.mote:nth-child(1) { top: 25%; left: 60%; animation-delay: 0s; }\n.mote:nth-child(2) { top: 40%; left: 70%; animation-delay: 2s; animation-duration: 15s; }\n.mote:nth-child(3) { top: 55%; left: 55%; animation-delay: 4s; animation-duration: 10s; }\n.mote:nth-child(4) { top: 30%; left: 75%; animation-delay: 6s; animation-duration: 14s; }\n.mote:nth-child(5) { top: 65%; left: 65%; animation-delay: 3s; animation-duration: 11s; }\n\n@keyframes float {\n  0%, 100% { \n    transform: translate(0, 0) scale(1);\n    opacity: 0.6;\n  }\n  25% { \n    transform: translate(-8px, 15px) scale(1.2);\n    opacity: 0.9;\n  }\n  50% { \n    transform: translate(5px, -10px) scale(0.8);\n    opacity: 0.5;\n  }\n  75% { \n    transform: translate(-3px, 8px) scale(1.1);\n    opacity: 0.8;\n  }\n}\n\n/* Steam wisps */\n.steam {\n  position: absolute;\n  bottom: 15%;\n  left: 50%;\n  transform: translateX(-50%);\n  width: 30px;\n  height: 50px;\n  opacity: 0.25;\n}\n\n.steam::before,\n.steam::after {\n  content: '';\n  position: absolute;\n  width: 8px;\n  height: 30px;\n  background: linear-gradient(to top, rgba(139,115,85,0.15), transparent);\n  border-radius: 50%;\n  animation: rise 4s eas